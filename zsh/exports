export TERM="xterm-256color"
export DISPLAY=:0.0
export EDITOR=vim

home_path=$HOME/bin
local_path=/usr/local/bin:/usr/local/sbin
export PATH=$home_path:$local_path:$PATH:/usr/lib/git-core
export NODE_PATH=$HOME/local/node:$HOME/local/node/lib/node_modules:$HOME/node_modules:$HOME/node_modules/less/lib
